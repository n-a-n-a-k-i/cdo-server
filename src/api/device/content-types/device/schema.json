{
  "kind": "collectionType",
  "collectionName": "devices",
  "info": {
    "singularName": "device",
    "pluralName": "devices",
    "displayName": "Device"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "model": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "vendor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vendor.vendor",
      "inversedBy": "devices"
    },
    "exposes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::expose.expose",
      "inversedBy": "devices"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
